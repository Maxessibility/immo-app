{
  "name": "immo-app",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev --prefix client\" \"npm:dev --prefix server\"",
    "dev:client": "npm --prefix client run dev",
    "dev:server": "npm --prefix server run dev",
    "build": "npm --prefix client run build && npm --prefix server run build"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
